{
    "id": "slidev",
    "version": "0.1.0",
    "name": "slidev",
    "description": "An environment for developing multiple slidev projects.",
    "documentationURL": "https://github.com/slidevjs/container",
    "licenseURL": "https://github.com/slidevjs/container/blob/main/LICENSE",
    "options": {
        "imageVariant": {
